    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=400",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run type-check && npm run lint",
    "test:deploy": "./test-before-deploy.sh",
    "test:quick": "npm run type-check && npm run build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:check": "npm run build && npm run start",
    "clean": "rm -rf .next node_modules/.cache",
    "lokalise:push": "node scripts/lokalise-sync.js push",
    "lokalise:sync": "node scripts/lokalise-sync.js sync"
